{
  "manifest_version": 1,

  "name": "YoutubeD",
  "description": "Youtube Downloader with axel.",
  "version": "1.0",

  "permissions": [
    "http://www.youtube.com/",
    "https://www.youtube.com/"
  ],

  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "YoutubeD"
  }
}
